{
    "comments": false,
    "sourceMaps": "both",
    "presets": [
        "@babel/preset-env",
        "@babel/preset-typescript"
    ],
    "plugins": [
        [
            "@babel/plugin-transform-runtime",
            {
                "corejs": false,
                "regenerator": true
            }
        ],
        "source-map-support"
    ]
}